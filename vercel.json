{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/config.json", "dest": "/public/config.json" },
    { "src": "/index.html", "dest": "/public/index.html" },
    { "src": "/icon.png", "dest": "/public/icon.png" },
    { "src": "/", "dest": "/public/index.html" }
  ]
}
